# ===================================================================
# Terraform Variables Example File
# Copia questo file in terraform.tfvars e compila con i tuoi valori
# ===================================================================

# AWS Configuration
aws_region = "eu-west-1"  # Cambia con la tua region preferita

# Project Configuration
project_name = "meta-catalog-api"
resource_prefix = "real-estate"  # Prefisso per tutte le risorse AWS

# API Key per autenticazione (genera una chiave sicura)
api_key_value = "YOUR_SECURE_API_KEY_HERE"

# Meta Business Configuration
meta_access_token = "YOUR_META_ACCESS_TOKEN"
meta_catalog_id   = "YOUR_CATALOG_ID"
meta_business_id  = "YOUR_BUSINESS_ID"
meta_app_id       = "YOUR_APP_ID"
meta_app_secret   = "YOUR_APP_SECRET"

# Esempio di valori (SOSTITUISCI CON I TUOI):
# api_key_value = "msk_live_1234567890abcdef1234567890abcdef"
# meta_access_token = "EAAG...your_long_token_here"
# meta_catalog_id = "841572311756772"
# meta_business_id = "123456789012345"
# meta_app_id = "123456789012345"
# meta_app_secret = "abcdef1234567890abcdef1234567890"